{
	"name": "gitterflow",
	"version": "0.1.0",
	"module": "src/index.ts",
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@types/bun": "latest",
		"lefthook": "^1.13.6",
		"lint-staged": "^16.2.4"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"bin": {
		"gitterflow": "./src/index.ts"
	},
	"scripts": {
		"prepare": "bunx lefthook install",
		"test": "bun test tests/commands/ tests/cli.test.ts",
		"test:unit": "bun test tests/commands/ tests/cli.test.ts",
		"test:integration": "bun test tests/integration/",
		"test:all": "bun test",
		"test:watch": "bun test --watch tests/commands/ tests/cli.test.ts",
		"test:coverage": "bun test --coverage",
		"check": "biome check src tests",
		"check:fix": "biome check src tests --write",
		"format": "biome format src tests --write",
		"lint": "biome lint src tests",
		"lint:fix": "biome lint src tests --write"
	},
	"type": "module"
}
